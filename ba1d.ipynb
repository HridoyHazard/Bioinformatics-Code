      "source": [
        "def patt(txt , k):\n",
        "  pos=[]\n",
        "  ss = \"\"\n",
        "  for i in range(len(k)-len(txt)+1):\n",
        "      if(k[i:i+len(txt)]==txt):\n",
        "        pos.append(i)\n",
        "  for x in pos:\n",
        "    if(x==0):\n",
        "      ss+=''+str(x)\n",
        "    else:\n",
        "      ss+=' '+str(x)\n",
        "  return ss\n",
        "  "
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "txt = input()\n",
        "k = input()\n",
        "print(patt(txt,k))\n",
        "\n"
