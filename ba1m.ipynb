      "source": [
        "def NumberToPattern(index, k):\n",
        "    bases = ['A', 'C', 'G', 'T']\n",
        "    pattern = ''\n",
        "    for i in range(k):\n",
        "        pattern += bases[index % 4]\n",
        "        index = index // 4\n",
        "    return pattern[::-1]"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "index = int(input())\n",
        "k = int(input())\n",
        "print(NumberToPattern(index, k))"
