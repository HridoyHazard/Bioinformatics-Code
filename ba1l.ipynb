      "source": [
        "def PatternToNumber(pattern):\n",
        "    seq = {'A':0, 'C':1, 'G':2, 'T':3}\n",
        "    k = len(pattern)\n",
        "    num = 0\n",
        "    for i in range(k):\n",
        "        num += seq[pattern[i]] * pow(4, k-i-1)\n",
        "    return num"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "pattern = input()\n",
        "print(PatternToNumber(pattern))"
      ],
